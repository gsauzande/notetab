{
  "name": "writend",
  "version": "0.1.0",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "description": "writend is the extension that will help you take the best and quickest notes on Chrome.",
  "permissions": ["tabs", "declarativeContent", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "img/writend_128.png"
  },
  "options_page": "options.html",
  "icons": {
    "16": "img/writend_16.png",
    "32": "img/writend_32.png",
    "48": "img/writend_48.png",
    "128": "img/writend_128.png"
  },
  "manifest_version": 2
}
